<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="bpmn_designer.GenericPanel" owl="1">
        <div class="mb-3">
           <label class="form-label fw-bold" for="bpmn-prop-name">名称 (Name)</label>
           <input type="text" class="form-control" id="bpmn-prop-name"
               t-att-value="businessObject.name"
               t-on-change="(ev) => this.updateProperty('name', ev.target.value)"
           />
        </div>
    </t>
</templates>